@model ListCRUD.Models.PersonalInfoViewModel.PersonalInfoCRUDViewModel


<div class="row">
    <div class="col-md-12">
        <table class="CustomTablePrimaryBlue">
            <tr>
                <th>ID</th>
                <td>@Model.Id</td>
            </tr>
            <tr>
                <th>First Name</th>
                <td>@Model.FirstName</td>
            </tr>
            <tr>
                <th>Last Name</th>
                <td>@Model.LastName</td>
            </tr>
            <tr>
                <th>Date Of Birth</th>
                <td>@Model.DateOfBirth.ToString("dddd, dd MMMM yyyy HH:mm:ss")</td>
            </tr>
            <tr>
                <th>City</th>
                <td>@Model.City</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>@Model.Country</td>
            </tr>
            <tr>
                <th>Mobile No</th>
                <td>@Model.MobileNo</td>
            </tr>
            <tr>
                <th>Pasport No</th>
                <td>@Model.PasportNo</td>
            </tr>
            <tr>
                <th>National ID No</th>
                <td>@Model.NID</td>
            </tr>

            <tr>
                <th>Created Date</th>
                <td>@Model.CreatedDate.ToString("dddd, dd MMMM yyyy HH:mm:ss")</td>
            </tr>
            <tr>
                <th>Modified Date</th>
                <td>@Model.ModifiedDate.ToString("dddd, dd MMMM yyyy HH:mm:ss")</td>
            </tr>
            <tr>
                <th>Created By</th>
                <td>@Model.CreatedBy</td>
            </tr>
            <tr>
                <th>Modifie By</th>
                <td>@Model.ModifiedBy</td>
            </tr>
        </table>
    </div>
</div>

